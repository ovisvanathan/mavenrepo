                                    javax.persistence.metamodel;resolution:=optional,
                                    javax.persistence.spi;resolution:=optional,
                                    org.eclipse.persistence.annotations;resolution:=optional,
                                    org.eclipse.persistence.config;resolution:=optional,
                                    org.eclipse.persistence.dbws;resolution:=optional,
                                    org.eclipse.persistence.descriptors;resolution:=optional,
                                    org.eclipse.persistence.descriptors.changetracking;resolution:=optional,
                                    org.eclipse.persistence.descriptors.copying;resolution:=optional,
                                    org.eclipse.persistence.descriptors.invalidation;resolution:=optional,
                                    org.eclipse.persistence.descriptors.partitioning;resolution:=optional,
                                    org.eclipse.persistence.dynamic;resolution:=optional,
                                    org.eclipse.persistence.eis.adapters;resolution:=optional,
                                    org.eclipse.persistence.eis.adapters.aq;resolution:=optional,
                                    org.eclipse.persistence.eis.adapters.jms;resolution:=optional,
                                    org.eclipse.persistence.eis.adapters.xmlfile;resolution:=optional,
                                    org.eclipse.persistence.eis.interactions;resolution:=optional,
                                    org.eclipse.persistence.eis.mappings;resolution:=optional,
                                    org.eclipse.persistence.exceptions;resolution:=optional,
                                    org.eclipse.persistence.exceptions.i18n;resolution:=optional,
                                    org.eclipse.persistence.expressions;resolution:=optional,
                                    org.eclipse.persistence.expressions.spatial;resolution:=optional,
                                    org.eclipse.persistence.history;resolution:=optional,
                                    org.eclipse.persistence.indirection;resolution:=optional,
                                    org.eclipse.persistence.internal;resolution:=optional,
                                    org.eclipse.persistence.internal.codegen;resolution:=optional,
                                    org.eclipse.persistence.internal.databaseaccess;resolution:=optional,
                                    org.eclipse.persistence.internal.dbws;resolution:=optional,
                                    org.eclipse.persistence.internal.descriptors;resolution:=optional,
                                    org.eclipse.persistence.internal.changetracking;resolution:=optional,
                                    org.eclipse.persistence.internal.dynamic;resolution:=optional,
                                    org.eclipse.persistence.internal.eis;resolution:=optional,
                                    org.eclipse.persistence.internal.eis.adapters;resolution:=optional,
                                    org.eclipse.persistence.internal.eis.adapters.aq;resolution:=optional,
                                    org.eclipse.persistence.internal.eis.adapters.jms;resolution:=optional,
