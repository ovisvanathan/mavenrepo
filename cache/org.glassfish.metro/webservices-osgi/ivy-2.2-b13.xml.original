                                    org.eclipse.persistence.internal.identitymaps;resolution:=optional,
                                    org.eclipse.persistence.internal.indirection;resolution:=optional,
                                    org.eclipse.persistence.internal.jaxb;resolution:=optional,
                                    org.eclipse.persistence.internal.jaxb.many;resolution:=optional,
                                    org.eclipse.persistence.internal.localization;resolution:=optional,
                                    org.eclipse.persistence.internal.localization.i18n;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.accessor;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.conversion;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.documentpreservation;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.record;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.record.deferred;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.record.namespaces;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.schema;resolution:=optional,
                                    org.eclipse.persistence.internal.oxm.schema.model;resolution:=optional,
                                    org.eclipse.persistence.internal.platform;resolution:=optional,
                                    org.eclipse.persistence.internal.platform.database;resolution:=optional,
                                    org.eclipse.persistence.internal.platform.database.oracle;resolution:=optional,
                                    org.eclipse.persistence.internal.platform.database.oracle.xdb;resolution:=optional,
                                    org.eclipse.persistence.internal.queries;resolution:=optional,
                                    org.eclipse.persistence.internal.security;resolution:=optional,
                                    org.eclipse.persistence.internal.sequencing;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.broadcast;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.corba;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.corba.sun;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.jms;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.rmi;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.rmi.iiop;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.factories;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.coordination.model;resolution:=optional,
                                    org.eclipse.persistence.internal.sessions.remote;resolution:=optional,
                                    org.eclipse.persistence.internal.weaving;resolution:=optional,
                                    org.eclipse.persistence.internal.xr;resolution:=optional,
                                    org.eclipse.persistence.internal.xr.sxf;resolution:=optional,
                                    org.eclipse.persistence.jaxb;resolution:=optional,
                                    org.eclipse.persistence.jaxb.attachment;resolution:=optional,
                                    org.eclipse.persistence.jaxb.compiler;resolution:=optional,
                                    org.eclipse.persistence.jaxb.dynamic;resolution:=optional,
                                    org.eclipse.persistence.jaxb.dynamic.metadata;resolution:=optional,
                                    org.eclipse.persistence.jaxb.javamodel;resolution:=optional,
                                    org.eclipse.persistence.jaxb.javamodel.oxm;resolution:=optional,
                                    org.eclipse.persistence.jaxb.javamodel.reflection;resolution:=optional,
                                    org.eclipse.persistence.jaxb.javamodel.xjc;resolution:=optional,
                                    org.eclipse.persistence.jaxb.metadata;resolution:=optional,
                                    org.eclipse.persistence.jaxb.xjc;resolution:=optional,
                                    org.eclipse.persistence.jaxb.xmlmodel;resolution:=optional,
                                    org.eclipse.persistence.logging;resolution:=optional,
                                    org.eclipse.persistence.mappings;resolution:=optional,
                                    org.eclipse.persistence.mappings.converters;resolution:=optional,
                                    org.eclipse.persistence.mappings.foundation;resolution:=optional,
                                    org.eclipse.persistence.mappings.querykeys;resolution:=optional,
                                    org.eclipse.persistence.mappings.structures;resolution:=optional,
                                    org.eclipse.persistence.mappings.transformers;resolution:=optional,
                                    org.eclipse.persistence.mappings.xdb;resolution:=optional,
                                    org.eclipse.persistence.oxm;resolution:=optional,
                                    org.eclipse.persistence.oxm.annotations;resolution:=optional,
                                    org.eclipse.persistence.oxm.attachment;resolution:=optional,
                                    org.eclipse.persistence.oxm.documentpreservation;resolution:=optional,
                                    org.eclipse.persistence.oxm.mappings;resolution:=optional,
                                    org.eclipse.persistence.oxm.mappings.converters;resolution:=optional,
                                    org.eclipse.persistence.oxm.mappings.nullpolicy;resolution:=optional,
