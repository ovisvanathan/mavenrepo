                                    org.eclipse.persistence.platform.database;resolution:=optional,
                                    org.eclipse.persistence.platform.server;resolution:=optional,
                                    org.eclipse.persistence.platform.xml;resolution:=optional,
                                    org.eclipse.persistence.queries;resolution:=optional,
                                    org.eclipse.persistence.sdo;resolution:=optional,
                                    org.eclipse.persistence.sdo.dataobjects;resolution:=optional,
                                    org.eclipse.persistence.sdo.helper;resolution:=optional,
                                    org.eclipse.persistence.sdo.helper.delegates;resolution:=optional,
                                    org.eclipse.persistence.sdo.helper.extension;resolution:=optional,
                                    org.eclipse.persistence.sdo.helper.jaxb;resolution:=optional,
                                    org.eclipse.persistence.sdo.helper.metadata;resolution:=optional,
                                    org.eclipse.persistence.sdo.types;resolution:=optional,
                                    org.eclipse.persistence.sequencing;resolution:=optional,
                                    org.eclipse.persistence.services;resolution:=optional,
                                    org.eclipse.persistence.services.glassfish;resolution:=optional,
                                    org.eclipse.persistence.tools;resolution:=optional,
                                    org.eclipse.persistence.tools.file;resolution:=optional,
                                    org.eclipse.persistence.tools.profiler;resolution:=optional,
                                    org.eclipse.persistence.tools.schemaframework;resolution:=optional,
                                    org.eclipse.persistence.tools.weaving.jpa;resolution:=optional,
                                    commonj.sdo.*;resolution:=optional,
                                    org.jvnet.staxex;resolution:=optional,
                                    org.w3c.dom;resolution:=optional,
                                    org.xml.sax;resolution:=optional,
                                    org.relaxng.datatype;resolution:=optional,
                                    *
                                </Import-Package>
                                <Private-Package>
                                    com.sun.xml.stream.*,
                                    com.sun.xml.xwss.*;version=${metro.release.version},
                                    javanet.staxutils.*
                                </Private-Package>
                                <DynamicImport-Package>
                                    org.glassfish.flashlight.provider
                                </DynamicImport-Package>
