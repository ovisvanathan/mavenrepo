		<!-- Tells Sonar to use JaCoCo as the code coverage tool -->
		<sonar.java.coveragePlugin>jacoco</sonar.java.coveragePlugin>
		<jacoco.dest.folder>${session.executionRootDirectory}/target</jacoco.dest.folder>
